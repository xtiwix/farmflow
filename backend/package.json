{
  "name": "farmflow-v3",
  "version": "3.0.0",
  "description": "FarmFlow - Farm Management System for Microgreens and Mushrooms",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "seed": "node scripts/seed.js",
    "test": "jest",
    "lint": "eslint .",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:reset": "npx sequelize-cli db:drop && npx sequelize-cli db:create && npm run seed"
  },
  "keywords": [
    "farm",
    "agriculture",
    "microgreens",
    "mushrooms",
    "management",
    "inventory",
    "orders"
  ],
  "author": "Bright Oasis Farm",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.35.2",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "sequelize-cli": "^6.6.2",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
